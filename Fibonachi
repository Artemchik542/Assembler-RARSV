

	li x17, 5
	ecall
	mv x11, x10
	li x17, 1
	li x10, 0
	li x12, 0
	blez x11, stop # если 0 и менее
	
	li x13, 1
	addi x11, x11, -1
	mv x10, x13
	beqz x11, stop # если 1

main_loop:
	mv x14, x12
	mv x12, x13
	mv x13, x14
	
	add x13, x12, x13
	addi x11, x11, -1
	
	bgtz x11, main_loop
	mv x10, x13
	
stop:
	ecall
